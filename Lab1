# Обчистлення першого прикладу
a <- 3 + 1/6 + 5.2
b <- (4.6 - 5/12) * 0.4
result1 <- a / b
print(result1)

# Обчистлення другого прикладу
result2 <- sqrt((2 - sqrt(7))^2) + sqrt((3 - sqrt(7))^2)
print(result2)

